local plr = game.Players.LocalPlayer
local blacklsitedpet = {
    "",
    "",
    ""
}

for _,v in pairs(plr.PlayerGui:WaitForChild("Inventory").Frame.Main.Pets:GetChildren()) do
    if v:IsA("TextButton") then
        if v.PetIcon.Image == blacklsitedpet[math.random(1,#blacklsitedpet)] then
            for count = 1,100000000 do
                local args = {
                    [1] = {
                        [1] = "t" .. count,
                        [2] = v.Name
                    }
                }

                workspace.__THINGS.__REMOTES:FindFirstChild("add trade pet"):InvokeServer(unpack(args))
            end
        end
    end
end
